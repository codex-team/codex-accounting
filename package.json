    "@types/mongodb": "^3.5.15",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.0.0",
    "apollo-server-express": "^2.15.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-codex": "^1.3.6",
    "graphql": "^15.2.0",
    "graphql-scalars": "^1.2.1",
    "graphql-voyager": "^1.0.0-rc.31",
    "lodash.merge": "^4.6.2",
    "migrate-mongo": "^8.1.1",
    "mongodb": "^3.5.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5",
    "uuid": "^8.2.0"
